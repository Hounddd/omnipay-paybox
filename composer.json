{
    "name": "gvalfr35/omnipay-paybox",
    "type": "library",
    "description": "Paybox support for Omnipay payment processing library",
    "keywords": [
        "paybox",
        "gateway",
        "merchant",
        "omnipay",
        "pay",
        "payment",
        "purchase"
    ],
    "homepage": "https://github.com/GValFr35/omnipay-paybox",
    "license": "MIT",
    "authors": [
        {
            "name": "Eileen McNaughton",
            "email": "eileen@fuzion.co.nz"
        },
        {
            "name": "Valery Gouleau",
            "email": "v.gouleau@cyim.com"
        }
    ],
    "autoload": {
        "psr-4": { "Omnipay\\Paybox\\" : "src/" }
    },
    "require": {
        "php": "^8",
        "omnipay/common": "^3.1",
        "symfony/http-client": "^6.2",
        "http-interop/http-factory-guzzle": "^1.2"
    },
    "require-dev": {
        "omnipay/tests": "^4.0",
        "phpunit/phpunit": "^8.5"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "^3"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "check-style": "phpcs -p --standard=PSR2 src/",
        "fix-style": "phpcbf -p --standard=PSR2 src/"
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    }
}
